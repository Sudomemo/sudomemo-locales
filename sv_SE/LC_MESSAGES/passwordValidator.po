msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Lösenordet du skrev matchade inte."

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "Lösenordet ska inte ha några ledande eller avslutande blanksteg."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Lösenorder borde vara mellan %d och %d tecken långt."

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Lösenorder borde ha minst en liten bokstav."

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Lösenordet kan inte matcha din email."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Lösenordet kan inte matcha ditt Flipnote Studio ID."

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Lösenorder borde innehålla ett nummer."

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "Lösenorder borde ha minst ett speciellt tecken."

msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "Lösenordet du skrev är för förutsägbart."

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Lösenordet du skrev kan inte skrivas med Flipnote Studio's tangentbord."

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "Lösenordet borde ha minst en stor bokstav."

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "Ditt lösenord får inte inhålla något av dessa tecken:"
