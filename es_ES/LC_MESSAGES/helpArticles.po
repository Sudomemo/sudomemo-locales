msgid "HOW_TO_POST"
msgstr "Cómo Publicar"

msgid "STEP_1_LOGIN"
msgstr "Desde el menú principal, toca \"Sala del Creador\" e inicia sesión."

msgid "STEP_2_NAVIGATE_CHANNELS"
msgstr "Regresa al menú principal y selecciona \"Canales.\""

msgid "STEP_3_SELECT_CHANNEL"
msgstr "Elige el canal que mejor se ajuste al tema de tu Flipnote."

msgid "STEP_4_UPLOAD_FLIPNOTE"
msgstr "Toca \"Publicar Flipnote\" en la esquina inferior derecha. Selecciona tu Flipnote. Bloquéalo (sin derivados) o déjalo desbloqueado (permite derivados)."

msgid "STEP_5_FINISH_SHARE"
msgstr "Confirma tu subida. ¡Tu Flipnote ya está compartido en Sudomemo!"
