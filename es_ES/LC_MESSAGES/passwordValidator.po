msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "La contraseña que ingresó se usa con demasiada frecuencia."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Tu contraseña no puede ser la misma que tu ID de Flipnote Studio."

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Tu contraseña no puede ser la misma que tu dirección de correo electronico."

msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Las contraseñas que ingresó no son las mismas."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Su contraseña debe tener entre %d y %d caracteres"

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Su contraseña no puede ser escrita en Flipnote Studio"

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "Su contraseña no puede contener estos caracteres:"

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "Su contraseña debe contener al menos 1 letra mayúscula"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Su contraseña debe contener al menos 1 letra minúscula"

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Su contraseña debe contener al menos 1 número"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "La contraseña debe contener al menos 1 carácter especial"

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "La contraseña no debe tener espacios al comienzo o al final."
