msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Wachtwoorden moeten tussen %d en %d karakters lang zijn."

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Wachtwoorden moeten minstens 1 kleine letter hebben"

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Je wachtwoord mag niet hetzelfde zijn als je email."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Je wachtwoord mag niet hetzelfde zijn als je Flipnote Studio ID."

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Wachtwoord moet minstens 1 nummer hebben"

