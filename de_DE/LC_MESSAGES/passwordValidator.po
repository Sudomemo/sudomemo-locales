msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "Das eingegebene Passwort wird zu häufig benutzt."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Dein Passwort darf nicht mit deiner Flipnote-Studio-ID übereinstimmen."

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Dein Passwort darf nicht mit deiner Email-Adresse übereinstimmen."

msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Die eingegebenen Passwörter stimmen nicht überein."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Passwortlänge sollte zwischen %d und %d Charakteren betragen"

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Das Passwort kann mit der Tastatur von Flipnote Studio nicht eingegeben werden."

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "Dein Passwort darf keines der folgenden Charaktere enthalten:"

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "Das Passwort soll min. 1 großgeschriebenen Buchstaben enthalten"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Das Passwort soll min. 1 kleingeschriebenen Buchstaben enthalten"

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Das Passwort soll min. 1 Ziffer enthalten"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "Das Passwort soll min. 1 Symbol enthalten"

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "Das Passwort soll keine nacheinander folgenden Leertaste-Eingaben enthalten."
