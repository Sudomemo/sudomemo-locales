msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "As palavras-passe que inseriste não combinam."

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "A palavra-passe não deve conter espaços dianteiros e traseiros."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "As palavras-passe devem ter entre %d e %d caracteres"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "A palavra-passe deve conter pelo menos, uma letra minúscula"

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "A tua palavra-passe não pode ser a mesma que o teu email."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "A tua palavra-passe não pode ser a mesma que o teu ID do Flipnote Studio."

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "A palavra-passe deve conter pelo menos, um número"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "A palavra-passe deve conter pelo menos, um caracter especial"

msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "A palavra-passe que inseriste é demasiado comum."

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Esta palavra-passe não pode ser escrita no teclado do Flipnote Studio."

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "A palavra-passe deve conter pelo menos, uma letra maiúscula"

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "A tua palavra-passe não pode conter nenhum destes caracteres:"
