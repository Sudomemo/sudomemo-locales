msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "Ο κωδικος που μας δωσατε χρησιμοποιειται υπερβολικα συχνα."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Ο κωδικος σας δεν μπορει να ειναι ο ιδιος με το Flipnote Studio ID σας."

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Ο κωδικος σας δεν μπορει να ναι ιδιος με το email σας."

msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Οι κωδικοι που βαλατε δεν ταιριαζουν."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Ο κωδικος πρεπει να εχει μηκος %d με %d χαρακτηρες"

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Ο κωδικος δενειναι δυνατος να γραφτει με το πληκτρολογιο του Flipnote Studio."

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "Ο κωδικος σας δεν μπορει να περιεχει καποιον απο αυτουςτους χαρακτηρες:"

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "Ο κωδικος πρεπει να περιεχει τουλαχιστον ενα κεφαλαιο γραμμα"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Ο κωδικος πρεπει να περιεχει τουλαχιστον ενα μικρο γραμμα"

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Ο κωδικος πρεπει να περιεχει τουλαχιστον εναν αριθμο"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "Ο κωδικος πρεπει να περιεχει τουλαχιστον εναν ειδικο χαρακτηρα"

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "Ο κωδικος δεν πρεπει να περιεχει κενα."
