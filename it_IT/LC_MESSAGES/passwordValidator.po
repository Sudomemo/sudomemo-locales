msgstr ""
"Language: it_IT\n"
"Content-Type: text/plain; charset=UTF-8\n"

msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "La password che hai inserito è troppo comune."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "La tua password e il tuo ID Flipnote Studio non possono essere identici."

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "La tua password e il tuo indirizzo email non possono essere identici."

msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Le password che hai inserito non corrispondono."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "La password deve contenere tra %d e %d caratteri di lunghezza"

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "La password non può essere digitata sulla tastiera di Flipnote Studio."

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "La tua password non può contenere nessuno di questi caratteri:"

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "La password deve contenere almeno 1 lettera maiuscola"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "La password deve contenere almeno 1 lettera minuscola"

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "La password deve contenere almeno 1 numero"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "La password deve contenere almeno 1 carattere speciale"

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "La password non deve contenere spazi vuoti iniziali o finali."
