msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "Le mot de passe que vous avez entré est trop couramment utilisé."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Votre mot de passe ne peut pas être identique à votre ID Flipnote Studio."

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Votre mot de passe ne peut pas être identique à votre e-mail."

msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Les mots de passe que vous avez entrés ne correspondent pas."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Le mot de passe doit comporter entre %d et %d caractères"

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Le mot de passe n'est pas saisissable au clavier de Flipnote Studio."

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "Le mot de passe ne peut contenir aucun de ces caractères :"

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "Le mot de passe doit contenir au moins 1 lettre en majuscule"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Le mot de passe doit contenir au moins 1 lettre en minuscule"

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Le mot de passe doit contenir au moins 1 chiffre"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "Le mot de passe doit contenir au moins 1 caractère spécial"

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "Le mot de passe ne doit pas avoir d'espace au début ou à la fin."


