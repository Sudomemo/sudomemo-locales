msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Wprowadzone hasla sie nie zgadzaja."

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "Haslo nie powinno miec spacji na poczatku lub na końcu."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Haslo powinno miec od %d do %d znaków dlugosci"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Haslo powinno zawierac przynajmniej jedna mala litere"

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Twoje haslo nie moze byc takie samo jak twój adres e-mail."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Twoje haslo nie moze byc takie samo jak twoje ID Flipnote Studio."

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Haslo powinno zawierac przynajmniej jedna cyfre"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "Haslo powinno zawierac przynajmniej jeden znak specjalny"

msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "Wprowadzone haslo jest zbyt czesto uzywane."

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Haslo nie mo≥ze byc wprowadzone na klawiaturze w Flipnote Studio."

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "Haslo powinno zawierac przynajmniej jedna duza litere"

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "Twoje haslo nie moze zawierac zadnego z nastepujacych znaków:"
