msgid "PASS_CHANGE_ERROR_CONFIRM_NO_MATCH"
msgstr "Wprowadzone hasła się nie zgadzają."

msgid "PASS_CHANGE_ERROR_LEAD_TRAIL_WHITESPACE"
msgstr "Hasło nie powinno mieć spacji na początku lub na końcu."

# Our passwords are hashed, the maxlength ensures they're usable on Flipnote Studio's soft keyboard
msgid "PASS_CHANGE_ERROR_LENGTH"
msgstr "Hasło powinno mieć od %d do %d znaków długości"

msgid "PASS_CHANGE_ERROR_LOWERCASE"
msgstr "Hasło powinno zawierać przynajmniej jedną małą literę"

msgid "PASS_CHANGE_ERROR_MATCHES_EMAIL"
msgstr "Twoje hasło nie może być takie samo jak twój adres e-mail."

msgid "PASS_CHANGE_ERROR_MATCHES_FSID"
msgstr "Twoje hasło nie może być takie samo jak twoje ID Flipnote Studio."

msgid "PASS_CHANGE_ERROR_NUMBER"
msgstr "Hasło powinno zawierać przynajmniej jedną cyfrę"

# this might not actually be enforced
msgid "PASS_CHANGE_ERROR_SPECIAL"
msgstr "Hasło powinno zawierać przynajmniej jeden znak specjalny"

msgid "PASS_CHANGE_ERROR_TOO_COMMON"
msgstr "Wprowadzone hasło jest zbyt często używane."

msgid "PASS_CHANGE_ERROR_TYPEABLE"
msgstr "Hasło nie mo≥że być wprowadzone na klawiaturze w Flipnote Studio."

msgid "PASS_CHANGE_ERROR_UPPERCASE"
msgstr "Hasło powinno zawierać przynajmniej jedną dużą literę"

msgid "PASS_CHANGE_RESTRICTED_CHARS"
msgstr "Twoje hasło nie może zawierać żadnego z następujących znaków:"
